<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none"
    >
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/identify_info"
            android:background="@color/grey2"
            android:padding="@dimen/body_desktop"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_height"
            android:background="@color/dividerColor"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/color_white"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/body_desktop"
            android:paddingBottom="@dimen/body_desktop"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="4"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:foregroundGravity="center"
                    style="@style/normal_text"
                    android:text="@string/colleges"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"></LinearLayout>
            </LinearLayout>
            <EditText
                android:id="@+id/college"
                android:layout_width="0dp"
                android:layout_weight="10"
                android:layout_height="wrap_content"
                style="@style/normal_hint_text"
                android:enabled="false"
                android:hint="浙江工业大学"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_height"
            android:background="@color/dividerColor"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/color_white"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/body_desktop"
            android:paddingBottom="@dimen/body_desktop"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="4"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:foregroundGravity="center"
                    style="@style/normal_text"
                    android:text="@string/number_ids"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"></LinearLayout>
            </LinearLayout>
            <EditText
                android:id="@+id/number"
                android:layout_width="0dp"
                android:layout_weight="10"
                android:layout_height="wrap_content"
                style="@style/normal_hint_text"
                android:hint="212033324223"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_height"
            android:background="@color/dividerColor"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/color_white"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/body_desktop"
            android:paddingBottom="@dimen/body_desktop"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="4"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:foregroundGravity="center"
                    style="@style/normal_text"
                    android:text="@string/names"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"></LinearLayout>
            </LinearLayout>
            <EditText
                android:id="@+id/name"
                android:layout_width="0dp"
                android:layout_weight="10"
                android:layout_height="wrap_content"
                style="@style/normal_hint_text"
                android:hint="王小毛"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_height"
            android:background="@color/dividerColor"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/color_white"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/body_desktop"
            android:paddingBottom="@dimen/body_desktop"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="4"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:foregroundGravity="center"
                    style="@style/normal_text"
                    android:text="@string/academy"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"></LinearLayout>
            </LinearLayout>
            <EditText
                android:id="@+id/academy"
                android:layout_width="0dp"
                android:layout_weight="10"
                android:layout_height="wrap_content"
                style="@style/normal_hint_text"
                android:textColorHint="@color/red1"
                android:hint="请填写学院"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_height"
            android:background="@color/dividerColor"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/color_white"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/body_desktop"
            android:paddingBottom="@dimen/body_desktop"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="4"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:foregroundGravity="center"
                    style="@style/normal_text"
                    android:text="@string/profession"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"></LinearLayout>
            </LinearLayout>
            <EditText
                android:id="@+id/profession"
                android:layout_width="0dp"
                android:layout_weight="10"
                android:layout_height="wrap_content"
                style="@style/normal_hint_text"
                android:textColorHint="@color/red1"
                android:hint="请填写专业"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_height"
            android:background="@color/dividerColor"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/color_white"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/body_desktop"
            android:paddingBottom="@dimen/body_desktop"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="4"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:foregroundGravity="center"
                    style="@style/normal_text"
                    android:text="@string/sex"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"></LinearLayout>
            </LinearLayout>
            <EditText
                android:id="@+id/sex"
                android:layout_width="0dp"
                android:layout_weight="10"
                android:layout_height="wrap_content"
                style="@style/normal_hint_text"
                android:hint="男"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_height"
            android:background="@color/dividerColor"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/color_white"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/body_desktop"
            android:paddingBottom="@dimen/body_desktop"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="4"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:foregroundGravity="center"
                    style="@style/normal_text"
                    android:text="@string/identifys"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"></LinearLayout>
            </LinearLayout>
            <EditText
                android:id="@+id/identify"
                android:layout_width="0dp"
                android:layout_weight="10"
                android:layout_height="wrap_content"
                style="@style/normal_hint_text"
                android:hint="本科生"/>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_height"
            android:background="@color/dividerColor"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:background="@color/color_white"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/body_desktop"
            android:paddingBottom="@dimen/body_desktop"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_height="match_parent">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="4"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textColor="@color/colorBlack"
                    android:foregroundGravity="center"
                    style="@style/normal_text"
                    android:text="@string/enter_college_time"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"></LinearLayout>
            </LinearLayout>
            <EditText
                android:id="@+id/time"
                android:layout_width="0dp"
                android:layout_weight="10"
                android:layout_height="wrap_content"
                style="@style/normal_hint_text"
                android:textColorHint="@color/red1"
                android:hint="请填写入学年份"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/line_height"
            android:background="@color/dividerColor"/>

        <Button
            android:id="@+id/next"
            style="@style/normal_button_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/btn_top"
            android:layout_marginLeft="@dimen/btn_left"
            android:layout_marginRight="@dimen/btn_right"
            android:layout_marginBottom="@dimen/btn_top"
            android:text="@string/confirm"/>

    </LinearLayout>
</ScrollView>